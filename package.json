      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "arrowParens": "always",
    "trailingComma": "all"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@types/bn.js": "^4.11.6",
    "@types/react-router-dom": "^5.1.7"
  },  
  "lint-staged": {
    "*.{js,jsx,css,md}": "prettier --write"
  }
